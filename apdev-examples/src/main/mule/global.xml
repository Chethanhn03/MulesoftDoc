		<http:listener-config name="HTTP_Listener_config" doc:name="HTTP Listener config" doc:id="4f2d104a-e973-4bbc-990b-975ffcf9b817" >
		<http:listener-connection host="0.0.0.0" port="${http.port}" />
	</http:listener-config>
	<http:request-config name="HTTP_Request_configuration" doc:name="HTTP Request configuration" doc:id="cb366a21-9601-472c-b8c8-152727dcd6c3" >
		<http:request-connection host="localhost" port="8081" />
	</http:request-config>
	<vm:config name="VM_Config" doc:name="VM Config" doc:id="a0119b30-cb26-4039-8843-a4508cc9e437" >
		<vm:queues >
			<vm:queue queueName="goodbye" />
		</vm:queues>
